SELECT [NAME] INTO :sColumnName FROM DBO.SYSCOLUMNSVIEW WHERE [TBNAME] = :sTabelle AND [TBCREATOR] = :SqlUser ORDER BY [Name] 

SELECT C.[Name] , P.[Feldart] FROM DBO.SYSCOLUMNSVIEW C , ProtKonfigFeld P INTO :sFeldname , :sFeldart WHERE C.[TBName] = :sTabelle AND P.[Tabelle] = C.[TBName] COLLATE DATABASE_DEFAULT  AND P.[Feldname] = C.[Name] AND P.[FeldArt] COLLATE DATABASE_DEFAULT  IN ( 'N' , 'S' , 'L' , 'Z' ) ORDER BY C.[ColNo] 

SELECT C.[Name] , P1.[Feldart] , CASE P1.[Feldart] WHEN 'N' THEN 1 WHEN 'S' THEN 1 WHEN 'L' THEN 1 WHEN 'Z' THEN 1 ELSE 2 END FROM ProtKonfigFeld P1 RIGHT OUTER JOIN DBO.SYSCOLUMNSVIEW C ON P1.[Tabelle] = C.[TBName] COLLATE DATABASE_DEFAULT AND P1.[Feldname] = C.[Name] COLLATE DATABASE_DEFAULT INTO :sFeldname , :sFeldart , :nOrder WHERE C.[TBName] = :sTabelle AND C.[ColType] NOT LIKE '%LONG%' ESCAPE '\' AND NOT EXISTS ( SELECT 2 FROM ProtKonfigFeld P2 WHERE P2.[Tabelle] = C.[TBName] COLLATE DATABASE_DEFAULT AND P2.[Feldname] = C.[Name] COLLATE DATABASE_DEFAULT AND P2.[FeldArt] = 'A' ) ORDER BY 3 , C.[ColNo] 

SELECT C.[Name] , P.[Feldart] , CASE P.[Feldart] WHEN 'N' THEN 1 WHEN 'S' THEN 1 WHEN 'L' THEN 1 WHEN 'Z' THEN 1 ELSE 2 END FROM DBO.SYSCOLUMNSVIEW C , ProtKonfigFeld P INTO :sFeldname , :sFeldart , :nOrder WHERE C.[TBName] = :sTabelle AND C.[ColType] NOT LIKE '%LONG%' ESCAPE '\' AND P.[Tabelle] = C.[TBName] COLLATE DATABASE_DEFAULT AND P.[Feldname] = C.[Name] COLLATE DATABASE_DEFAULT AND P.[FeldArt] NOT IN ( 'A' , 'F' ) ORDER BY 3 , C.[ColNo] 

SELECT [ColType] , [Length] FROM DBO.SYSCOLUMNSVIEW INTO :sTyp , :nLaenge WHERE [TbName] = :sTabelle AND [Name] = :sFeldname 

SELECT [Name] , [Coltype] , [Length] , [Scale] , [Nulls] FROM DBO.SYSCOLUMNSVIEW where tbname=" + locals.sHochkomma + locals.sTabelleAlt + locals.sHochkomma + " AND TbCreator = @UPPER(:SqlUser) " + " Into :sName, :sColtype, :nLength, :nScale, :sNulls ORDER BY ColNo

SELECT [COLTYPE] , [LENGTH] , [SCALE] , [NULLS] FROM DBO.SYSCOLUMNSVIEW INTO :sType , :nTLaenge , :nTScale , :sTNulls WHERE [TBNAME] = UPPER ( :sTabelle ) AND [Name] = UPPER ( :sSpalte ) 

SELECT C.Tbname,  C.Name
FROM DBO.SYSCOLUMNSVIEWC, INFORMATION_SCHEMA.TABLES T
WHERE C.tbname = T.Table_Name AND T.TABLE_TYPE ='BASE TABLE' AND T.TABLE_SCHEMA =:sSchema
AND C.Name
IN ('ARTNR','ARTIKELNR','TEILENR','APLNR','STULINR','STUELINR','MATERIALNR','STLAPLNR'
  ,'BETRMITTELNR','WERKZEUGNR','WKZNR','EINSATZ','ERGAENZUNGSNR'
  ,'ZUSATZSTOFFNR1','ZUSATZSTOFFNR2','ZUSATZSTOFFNR3','ZUSATZSTOFFNR4','REGENERAT' )
UNION
SELECT C.Tbname,  C.Name
FROM DBO.SYSCOLUMNSVIEW C, INFORMATION_SCHEMA.TABLES T
WHERE C.tbname = T.Table_Name AND T.TABLE_TYPE ='T'  AND T.TABLE_SCHEMA =:sSchema
AND (   (C.Name ='RABATTNR1' AND tbname not like 'EINRECHKO%' )
OR(C.Name IN('SETNR','SETTEIL') AND TbName !='AUFTRPOS' )
OR (C.Name IN('VERPACKNR') AND TbName NOT IN('TMP_AFORUECK','AFORUECK','PLANSTAFFELN','PLANZAHLEN')	 )
OR (C.Name IN('MATNR') AND TbName NOT IN('INTRAKONFIG')	 ))
INTO :tblAUM_TableMitArtNr.colAUM_TbName, :tblAUM_TableMitArtNr.colAUM_ColName

SELECT C.Tbname,  C.Name
FROM DBO.SYSCOLUMNSVIEWC, INFORMATION_SCHEMA.TABLES T
WHERE C.tbname = T.Table_Name AND T.TABLE_TYPE ='BASE TABLE' AND T.TABLE_SCHEMA =:sSchema
AND C.Name
IN ('ARTNR','ARTIKELNR','TEILENR','APLNR','STULINR','STUELINR','MATERIALNR','MATNR','STLAPLNR'
,'BETRMITTELNR','WERKZEUGNR','WKZNR','EINSATZ','ERGAENZUNGSNR'
,'ZUSATZSTOFFNR1','ZUSATZSTOFFNR2','ZUSATZSTOFFNR3','ZUSATZSTOFFNR4','REGENERAT' )
UNION
SELECT C.Tbname,  C.Name
FROM DBO.SYSCOLUMNSVIEW C, INFORMATION_SCHEMA.TABLES T
WHERE C.tbname = T.Table_Name AND T.TABLE_TYPE ='T'  AND T.TABLE_SCHEMA =:sSchema
AND (    (C.Name ='RABATTNR1' AND tbname !='EINRECHKO')
OR (C.Name IN('SETNR','SETTEIL') AND TbName !='AUFTRPOS')
OR (C.Name IN('VERPACKNR') AND TbName NOT IN('TMP_AFORUECK','AFORUECK','PLANSTAFFELN','PLANZAHLEN')	)
OR (C.Name IN('MATNR') AND TbName NOT IN('INTRAKONFIG')	))
INTO :tblSPS_Tabellen.colSPS_Namen, :tblSPS_Tabellen.colSPS_Attribut

SELECT [Name] , CASE [ColType] WHEN 'CHAR' THEN 'Char' WHEN 'VARCHAR' THEN 'Char' WHEN 'LONGVAR' THEN 'LongChar' WHEN 'FLOAT' THEN 'Numeric' WHEN 'INTEGER' THEN 'Numeric' WHEN 'SMALLINT' THEN 'Numeric' WHEN 'DECIMAL' THEN 'Numeric' WHEN 'TIMESTMP' THEN 'Date' WHEN 'TIME' THEN 'Date' WHEN 'DATE' THEN 'Date' END FROM DBO.SYSCOLUMNSVIEW 
INTO	:sColName,
:sColType
WHERE TbName = '" + psTable.ToUpper() + "'
ORDER BY ColNo"

SELECT [Name] FROM dbo.SysTables INTO :sTable WHERE [Name] LIKE 'QUER%' ESCAPE '\' ORDER BY [Name] DESC 
